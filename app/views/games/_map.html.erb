<div class="map-board">
  <div class="fields">
    <% fields.each.with_index do |field, index| %>
      <%= render partial: 'field', locals: { field: field } %>
    <% end %>
  </div>
  <div class="places">
    <% places.each.with_index do %>
      <div class="place"></div>
      <div class="place-background"></div>
    <% end %>
  </div>
</div>
