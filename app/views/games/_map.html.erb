<div class="map-board">
  <div class="fields">
    <% fields.each.with_index do |field, index| %>
      <%= render partial: 'field', locals: { field: field } %>
    <% end %>
  </div>
  <div class="places">
    <% places.each do |place| %>
      <%= render partial: 'place', locals: { place: place } %>
    <% end %>
  </div>
  <div class="roads">
    <% roads.each do |road| %>
      <%= render partial: 'road', locals: { road: road } %>
    <% end %>
  </div>
</div>
